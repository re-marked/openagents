{
  "agents": {
    "list": [
      {
        "id": "main",
        "name": "Main",
        "default": true,
        "workspace": "/data/workspace"
      }
    ],
    "defaults": {
      "model": {
        "primary": "google/gemini-2.5-flash"
      },
      "sandbox": {
        "mode": "off"
      }
    }
  },
  "gateway": {
    "mode": "local",
    "port": 18789,
    "bind": "lan",
    "auth": {
      "mode": "token"
    },
    "controlUi": {
      "dangerouslyAllowHostHeaderOriginFallback": true
    },
    "http": {
      "endpoints": {
        "chatCompletions": {
          "enabled": true
        },
        "responses": {
          "enabled": true
        }
      }
    }
  }
}
